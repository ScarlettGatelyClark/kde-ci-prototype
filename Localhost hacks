Localhost hacks

cd ~/build-kde-org
/bin/bash update-setup.sh
sed s/build.kde.org[[:space:]]create-job/localhost[[:space:]]create-job/ $HOME/scripts/jenkins/add-new-job.sh -i
sed s/build.kde.org/localhost/ $HOME/scripts/config/build/global.cfg -i
sed s/opSys=linux/opSys=ubuntu/ $HOME/scripts/config/build/linux64-g++.cfg -i
sed s/local=False/local=True/ $HOME/scripts/tools/kdecilib.py -i
sed '/ dogtail /s/^/#/' $HOME/scripts/config/base/kf5-qt5 -i
sed '/ dogtail /s/^/#/' $HOME/scripts/config/base/oldstable-qt4 -i
sed '/ dogtail /s/^/#/' $HOME/scripts/config/base/latest-qt4 -i
sed '/ dogtail /s/^/#/' $HOME/scripts/config/base/stable-qt4 -i
sed '/ createRemotePathCommand /s/^/#/' $HOME/scripts/config/build/global.cfg -i
